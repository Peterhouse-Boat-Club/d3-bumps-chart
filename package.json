{
  "name": "d3-bumps-chart",
  "version": "0.12.1",
  "description": "Draw bumps charts",
  "keywords": [
    "d3",
    "d3-module",
    "bumps"
  ],
  "homepage": "https://github.com/johnwalley/d3-bumps-chart/",
  "license": "BSD-3-Clause",
  "author": {
    "name": "John Walley",
    "url": "http://www.walley.org.uk/"
  },
  "main": "build/d3-bumps-chart.js",
  "module": "build/d3-bumps-chart.js",
  "jsnext:main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnwalley/d3-bumps-chart.git"
  },
  "scripts": {
    "pretest": "rm -rf build && mkdir build && rollup -c --banner \"$(preamble)\" -f umd -n d3 -o build/d3-bumps-chart.js --extend -- index.js",
    "test": "tape test/**/*-test.js && eslint index.js src",
    "prepublish": "npm run test && uglifyjs --beautify \"preamble='$(preamble)'\" build/d3-bumps-chart.js -c -m -o build/d3-bumps-chart.min.js",
    "postpublish": "zip -j build/d3-bumps-chart.zip -- LICENSE README.md build/d3-bumps-chart.js build/d3-bumps-chart.min.js"
  },
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-dispatch": "^1.0.3",
    "d3-dsv": "^1.0.8",
    "d3-scale": "^2.0.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.1",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.10"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "jsdom": "^11.10.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tape": "^4.9.0",
    "uglify-js": "^3.3.23"
  }
}
